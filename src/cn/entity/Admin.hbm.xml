<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="cn.entity.Admin" table="Admin">
		<id name="a_id" column="a_id">
			<generator class="native"></generator>
		</id>
		<property name="a_number" column="a_number"></property>
		<property name="a_pass" column="a_pass"></property>
		<property name="a_name" column="a_name"></property>
		
	</class>
</hibernate-mapping>